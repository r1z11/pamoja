{
  "expo": {
    "name": "Pamoja",
    "slug": "pamoja",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pamoja.app"
    },
    "description": "Pamoja Wallet",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SpaceMono-Regular.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "space-mono",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/SpaceMono-Regular.ttf",
                    "weight": "normal"
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/SpaceMono-Regular.ttf"
            ]
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c9ccf879-6836-4c6d-b355-4d53cefbf524"
      }
    }
  }
}
